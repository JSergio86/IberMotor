                    app:layout_constraintEnd_toEndOf="@+id/cuadroInfoCocheSkyline"
                    app:layout_constraintStart_toStartOf="@+id/cuadroInfoCocheSkyline"
                    app:layout_constraintTop_toBottomOf="@+id/nombreInfolinearSkyline" />

            </androidx.constraintlayout.widget.ConstraintLayout>


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintSilvia"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="20dp">
            <View
                android:id="@+id/cuadroInfoCocheSilvia"
                android:layout_width="339dp"
                android:layout_height="329dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="36dp"
                android:layout_marginEnd="36dp"
                android:background="@drawable/rectangle_1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                />


            <ImageView
                android:id="@+id/fotoCocheSilvia"
                android:layout_width="340dp"
                android:layout_height="160dp"
                android:scaleType="centerCrop"
                android:src="@drawable/silvia"
                app:layout_constraintEnd_toEndOf="@+id/cuadroInfoCocheSilvia"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/cuadroInfoCocheSilvia"
                app:layout_constraintTop_toTopOf="@+id/cuadroInfoCocheSilvia"
                tools:ignore="MissingConstraints" />

            <LinearLayout
                android:id="@+id/preciolinearSilvia"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="14dp"
                android:layout_marginTop="10dp"
                android:orientation="vertical"

                app:layout_constraintStart_toStartOf="@id/cuadroInfoCocheSilvia"
                app:layout_constraintTop_toBottomOf="@+id/fotoCocheSilvia">

                <TextView
                    android:id="@+id/precioTextTitleSilvia"
                    android:layout_width="81dp"
                    android:layout_height="18dp"
                    android:text="Precio total"
                    android:textAppearance="@style/precio_tota"
                    android:textSize="40px" />

                <TextView
                    android:id="@+id/precioTextSilvia"
                    android:layout_width="80dp"
                    android:layout_height="22dp"
