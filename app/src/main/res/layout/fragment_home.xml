<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="SpeakableTextPresentCheck">


    <ImageView
        android:id="@+id/perfil"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:scaleType="centerCrop"
        android:src="@drawable/perfil"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />


    <EditText
        android:id="@+id/barraBusqueda"
        android:layout_width="346dp"
        android:layout_height="42dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/barrabusqueda"
        android:drawableStart="@drawable/iconobuscar"
        android:drawablePadding="10dp"
        android:ems="10"
        android:hint="Buscar"
        android:paddingStart="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/perfil" />


    <ImageButton
        android:id="@+id/iconoFiltro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="28dp"
        android:background="@null"
        android:src="@drawable/iconofiltro"
        app:layout_constraintBottom_toBottomOf="@+id/barraBusqueda"
        app:layout_constraintRight_toRightOf="@id/barraBusqueda"
        app:layout_constraintTop_toTopOf="@+id/barraBusqueda"
        />

    <RelativeLayout
        android:id="@+id/iconosAutomoviles"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="left|center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/barraBusqueda">

        <ImageView
            android:id="@+id/iconoCoche"
            android:layout_width="38dp"
            android:layout_height="25dp"
            android:src="@drawable/iconitocoche" />


        <ImageView
            android:id="@+id/iconoFurgoneta"
            android:layout_width="38dp"
            android:layout_height="25dp"
            android:layout_alignTop="@id/iconoCoche"
            android:layout_marginStart="20dp"
            android:layout_toEndOf="@id/iconoCoche"
            android:src="@drawable/iconitofurgoneta" />


    </RelativeLayout>


    <ImageView
        android:id="@+id/iconoVista"
        android:layout_width="37dp"
        android:layout_height="33dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="8dp"
        android:scaleType="centerCrop"
        android:src="@drawable/cambiarvista"
        app:layout_constraintStart_toEndOf="@+id/iconosAutomoviles"
        app:layout_constraintTop_toBottomOf="@+id/barraBusqueda"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/menuDrawer"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:scaleType="centerCrop"
        android:src="@drawable/iconodrawer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />



    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iconosAutomoviles"
        tools:ignore="SpeakableTextPresentCheck">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_gravity="center_horizontal">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/bmwConstraint"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="20dp">
                <View
                    android:id="@+id/cuadroInfoCoche"
                    android:layout_width="339dp"
                    android:layout_height="329dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="36dp"
                    android:layout_marginEnd="36dp"
                    android:background="@drawable/rectangle_1"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.509"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="MissingConstraints,NotSibling" />

                <ImageView
                    android:id="@+id/fotoCoche"
                    android:layout_width="340dp"
                    android:layout_height="160dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/bmw"
                    app:layout_constraintEnd_toEndOf="@+id/cuadroInfoCoche"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="@+id/cuadroInfoCoche"
                    app:layout_constraintTop_toTopOf="@+id/cuadroInfoCoche"
                    tools:ignore="MissingConstraints" />

                <LinearLayout
                    android:id="@id/preciolinear"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="14dp"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical"

                    app:layout_constraintStart_toStartOf="@id/cuadroInfoCoche"
                    app:layout_constraintTop_toBottomOf="@+id/fotoCoche">

                    <TextView
                        android:id="@+id/precioTextTitle"
                        android:layout_width="81dp"
                        android:layout_height="18dp"
                        android:text="Precio total"
                        android:textAppearance="@style/precio_tota"
                        android:textSize="40px" />

                    <TextView
                        android:id="@+id/precioText"
                        android:layout_width="80dp"
                        android:layout_height="22dp"
                        android:textStyle="bold"
                        android:text="23.999€"
                        android:textAppearance="@style/some_id" />
                </LinearLayout>


                <LinearLayout
                    android:id="@id/nombreInfolinear"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="14dp"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical"
                    app:layout_constraintStart_toStartOf="@id/cuadroInfoCoche"
                    app:layout_constraintTop_toBottomOf="@+id/preciolinear">

                    <TextView
                        android:id="@+id/nombreText"
                        android:layout_width="199dp"
                        android:layout_height="23dp"
                        android:text="BMW Serie 3 M3 E36"
                        android:textAppearance="@style/nameVistaPrincipal"
                        app:layout_constraintStart_toStartOf="@+id/cuadroInfoCoche"
                        app:layout_constraintTop_toBottomOf="@+id/preciolinear" />

                    <RelativeLayout
                        android:id="@+id/atributosLayout"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="left|center">


                        <TextView
                            android:id="@+id/ciudadText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginRight="2dp"
                            android:text="Barcelona -"
                            android:textAppearance="@style/styleGris" />


                        <TextView
                            android:id="@+id/kilometrosText"
                            android:layout_width="wrap_content"
                            android:layout_height="16dp"
                            android:layout_marginRight="2dp"
                            android:layout_toRightOf="@id/ciudadText"
                            android:text="217.000km -"
                            android:textAppearance="@style/styleGris" />


                        <TextView
                            android:id="@+id/añosText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="2dp"
                            android:layout_toRightOf="@id/kilometrosText"
                            android:text="1997 -"
                            android:textAppearance="@style/styleGris" />

                        <TextView
                            android:id="@+id/combustibleText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="2dp"
                            android:layout_toRightOf="@id/añosText"
                            android:text="Gasolina"
                            android:textAppearance="@style/styleGris" />

                    </RelativeLayout>
                </LinearLayout>


                <TextView
                    android:id="@+id/garantia"
                    android:layout_width="120dp"
                    android:layout_height="14dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="14dp"
                    android:layout_marginTop="13dp"
                    android:layout_marginEnd="205dp"
                    android:gravity="top"
                    android:lineSpacingExtra="-5sp"
                    android:text="Garantia 1 año"
                    android:textAppearance="@style/garantia"
                    app:layout_constraintEnd_toEndOf="@+id/cuadroInfoCoche"
                    app:layout_constraintStart_toStartOf="@+id/cuadroInfoCoche"
                    app:layout_constraintTop_toBottomOf="@+id/nombreInfolinear" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintDodge"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="20dp">
            <View
