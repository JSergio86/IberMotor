                <TextView
                    android:id="@+id/precioTextSkyline"
                    android:layout_width="80dp"
                    android:layout_height="22dp"
                    android:textStyle="bold"
                    android:text="35.999€"
                    android:textAppearance="@style/some_id" />
            </LinearLayout>

                <LinearLayout
                    android:id="@+id/nombreInfolinearSkyline"
