                            android:textAppearance="@style/styleGris"
                            />

                        <TextView
                            android:id="@+id/añosTextSkyline"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="2dp"
                            android:layout_toRightOf="@id/kilometrosTextSkyline"
