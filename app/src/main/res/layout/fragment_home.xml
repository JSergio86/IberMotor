                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginRight="2dp"
                        android:text="Badalona -"
                        android:textAppearance="@style/styleGris" />


                    <TextView
                        android:id="@+id/kilometrosTextDodge"
                        android:layout_width="wrap_content"
                        android:layout_height="16dp"
                        android:layout_marginRight="2dp"
                        android:layout_toRightOf="@id/ciudadTextDodge"
                        android:text="93.000km -"
                        android:textAppearance="@style/styleGris" />


                    <TextView
                        android:id="@+id/añosTextDodge"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="2dp"
                        android:layout_toRightOf="@id/kilometrosTextDodge"
                        android:text="2016 -"
                        android:textAppearance="@style/styleGris" />

                    <TextView
                        android:id="@+id/combustibleTextDodge"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="2dp"
                        android:layout_toRightOf="@id/añosTextDodge"
                        android:text="Gasolina"
                        android:textAppearance="@style/styleGris" />

                </RelativeLayout>
            </LinearLayout>
            <TextView
                android:id="@+id/garantiaDodge"
                android:layout_width="120dp"
                android:layout_height="14dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="14dp"
                android:layout_marginTop="13dp"
                android:layout_marginEnd="205dp"
                android:gravity="top"
                android:lineSpacingExtra="-5sp"
                android:text="Garantia 1 año"
                android:textAppearance="@style/garantia"
                app:layout_constraintEnd_toEndOf="@+id/cuadroInfoCocheDodge"
                app:layout_constraintStart_toStartOf="@+id/cuadroInfoCocheDodge"
                app:layout_constraintTop_toBottomOf="@+id/nombreInfolinearDodge" />
            </androidx.constraintlayout.widget.ConstraintLayout>


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintSkyline"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="20dp">
            <View
                android:id="@+id/cuadroInfoCocheSkyline"
                android:layout_width="339dp"
                android:layout_height="329dp"
