                    android:text="35.999€"
                    android:textAppearance="@style/some_id" />
            </LinearLayout>
                <LinearLayout
                    android:id="@+id/nombreInfolinearSilvia"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="14dp"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical"
                    app:layout_constraintStart_toStartOf="@id/cuadroInfoCocheSilvia"
                    app:layout_constraintTop_toBottomOf="@+id/preciolinearSilvia">


                    <TextView
                        android:id="@+id/nombreTextSilvia"
                        android:layout_width="199dp"
                        android:layout_height="wrap_content"
                        android:text="Nissan Silvia"
                        android:textAppearance="@style/nameVistaPrincipal" />

                    <RelativeLayout
                        android:id="@+id/atributosLayoutSilvia"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="left|center">

                        <TextView
                            android:id="@+id/ciudadTextSilvia"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginRight="2dp"
                            android:text="Madrid -"
                            android:textAppearance="@style/styleGris" />

                        <TextView
                            android:id="@+id/kilometrosTextSilvia"
                            android:layout_width="wrap_content"
                            android:layout_height="16dp"
                            android:layout_marginRight="2dp"
                            android:layout_toRightOf="@id/ciudadTextSilvia"
                            android:text="10.000km -"
                            android:textAppearance="@style/styleGris" />

                        <TextView
                            android:id="@+id/añosTextSilvia"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="2dp"
                            android:layout_toRightOf="@id/kilometrosTextSilvia"
                            android:text="1995 -"
                            android:textAppearance="@style/styleGris" />

                        <TextView
                            android:id="@+id/combustibleTextSilvia"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="2dp"
                            android:layout_toRightOf="@id/añosTextSilvia"
                            android:text="Gasolina"
                            android:textAppearance="@style/styleGris" />

                    </RelativeLayout>
                </LinearLayout>
                <TextView
                    android:id="@+id/garantiaSilvia"
                    android:layout_width="120dp"
                    android:layout_height="14dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginStart="14dp"
                    android:layout_marginTop="13dp"
                    android:layout_marginEnd="205dp"
